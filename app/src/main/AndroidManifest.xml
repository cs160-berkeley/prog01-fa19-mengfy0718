<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.evcharger">

    <application
        android:name=".Settings"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ChargingActivity"
            android:label="Charging Now"
            android:parentActivityName=".MainActivity"/>
        <activity
            android:name=".ScheduleConfirm"
            android:label="Schedule Confirmed"
            android:parentActivityName=".ScheduleActivity" />
        <activity
            android:name=".SettingsActivity"
            android:label="Settings"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".CalculatorActivity"
            android:label="Charging Time Calculator"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".ScheduleActivity"
            android:label="Schedule"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>